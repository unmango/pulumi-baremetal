{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "command.ts",
        "config/index.ts",
        "config/vars.ts",
        "coreutils/chmod.ts",
        "coreutils/index.ts",
        "coreutils/mkdir.ts",
        "coreutils/mktemp.ts",
        "coreutils/mv.ts",
        "coreutils/rm.ts",
        "coreutils/tar.ts",
        "coreutils/tee.ts",
        "coreutils/wget.ts",
        "index.ts",
        "kubeadm/index.ts",
        "kubeadm/kubeadm.ts",
        "provider.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
